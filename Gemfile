# frozen_string_literal: true

source "https://rubygems.org"
gemspec

# Including GitHub Pages gem
# explicit jekyll 4 dependencies to support ruby 3
gem "jekyll", "~> 4.3"

# Plugins
group :jekyll_plugins do
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
  gem "jekyll-remote-theme"
  gem 'jemoji'
  gem 'jekyll-scholar'
  gem 'faraday', '>= 1.0'
  gem 'webrick'
end

